set term wxt font "Helvetica,18"

plot "eigenv_cyc_10_0.00_0.00_1.00" u 1:2 t "d = 10" w l lw 3.0 lc 'blue',\
     "eigenv_cyc_50_0.00_0.00_1.00" u 1:2 t "d = 50" w l lw 3.0 lc 'brown', \
     "eigenv_cyc_1000_0.00_0.00_1.00" u 1:2 t "d = 1000" w l lw 3.0 lc 'green'
set xrange [0:1]
set title "Eigenstates energy vs quantum number - delta = 1.00"
set xlabel "Quantum number"
set ylabel "Energy"
set key box lw 2
set key left
