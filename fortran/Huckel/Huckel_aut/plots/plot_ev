set term wxt font "Helvetica,18"

plot "eigenv_lin_10_0.00_0.00_1.00" u 1:2 t "d=10" w l lw 2.0 lc 'blue',\
     "eigenv_lin_20_0.00_0.00_1.00" u 1:2 t "d = 20" w l lw 2.0 lc 'brown', \
     "eigenv_lin_100_0.00_0.00_1.00" u 1:2 t "d = 100" w l lw 2.0 lc 'brown'
set xrange [0:1]
set title "Eigenstates energy vs quantum number - delta = 1.00"
set xlabel "Quantum number"
set ylabel "Energy"
set key box lw 2
set key left
